{
  "name": "env-cmd-file-example",
  "version": "9.0.0",
  "description": "env-cmd env file examples",
  "author": "Todd Bluhm",
  "license": "MIT",
  "scripts": {
    "test": "npm run test-basic && npm run test-custom-path && npm run test-shell-script && npm run test-use-shell && npm run test-async-file",
    "test-basic": "env-cmd node index.js",
    "test-custom-path": "env-cmd -f ./.env-file node index.js",
    "test-shell-script": "env-cmd -f ./shell-script.env sh ./env.sh",
    "test-use-shell": "env-cmd -f ./use-shell.env --use-shell \"node index.js && sh ./env.sh\"",
    "test-async-file": "env-cmd -f ./async-env.js node index.js"
  },
  "dependencies": {
    "chalk": "^2.0.0",
    "env-cmd": "^10.0.0"
  }
}
